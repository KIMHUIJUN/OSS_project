<%= form_with(model: commend, local: true) do |form| %>
  

  <div class="field">
    <%= form.label :commend %>
    <%= form.text_field :commend %>
  </div>

  <div class="field">
    <%= form.hidden_field :user_id , value: current_user.id%>
  </div>

  <div class="field">
    <%= form.hidden_field :album_id, value: @album.id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
